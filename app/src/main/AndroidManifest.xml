<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:versionCode="14"
    android:versionName="2.9">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_MEDIA_AUDIO" />
    <uses-permission android:name="android.permission.READ_MEDIA_IMAGES" />
    <uses-permission android:name="android.permission.READ_MEDIA_VIDEO" />

    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:enableOnBackInvokedCallback="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CrossEPS"
        tools:targetApi="tiramisu">
        <activity
            android:name=".Logo"
            android:exported="false" />
        <activity
            android:name=".ListePDF"
            android:label="@string/title_activity_liste_p_d_f"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.CrossEPS.NoActionBar" />
        <activity
            android:name=".CreerListe"
            android:label="@string/title_activity_creer_liste"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.CrossEPS.NoActionBar" />
        <activity
            android:name=".CreerUn"
            android:label="@string/title_activity_creer_un"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.CrossEPS.NoActionBar" />
        <activity
            android:name=".CreerDossard"
            android:label="@string/title_activity_creer_dossard"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.CrossEPS.NoActionBar" />
        <activity
            android:name=".ListeCSV"
            android:label="@string/title_activity_liste_c_s_v"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.CrossEPS.NoActionBar" />
        <activity
            android:name=".Resultats"
            android:label="@string/title_activity_resultats"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.CrossEPS.NoActionBar" />
        <activity
            android:name=".Course"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.CrossEPS.NoActionBar"></activity>
        <activity
            android:name=".Accueil"
            android:exported="true"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="portrait"
            android:stateNotNeeded="true"
            tools:replace="android:screenOrientation" />

        <provider
            android:name=".MonFileProvider"
            android:authorities="fr.kunze.crossepsv2.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/providers_path" />
        </provider>
    </application>

</manifest>